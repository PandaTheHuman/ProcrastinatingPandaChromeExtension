{
  "manifest_version": 2,

  "name": "PROCRASTINATION PANDA",
  "description": "change all images to scolding panda",
  "version": "1.0",

  "icons": {
    "48": "panda-icon.png"
  },

  "permissions": ["tabs", "<all_urls>"],

  "browser_action": {
    "default_icon": "panda-icon.png",
    "default_popup": "popup.html"
  }
}
